<head>
  <link rel="stylesheet" href="/style.css" />
</head>

<h1 class="feed-title">Create Post</h1>

<form class="post-form" method="POST" action="/feed/create">
  <textarea
    name="content"
    class="post-textarea"
    placeholder="Write something..."
    required
  ></textarea>
  <button type="submit" class="post-btn">Post</button>
</form>
